{
    <div id="ClosedDiv" style="height: 100%; width: 100%; background-color: grey; position: fixed; opacity: 0.90; z-index: 3147483648; display: block; clear: both;">
        <div style="width: 70%; text-align: center; margin: auto; margin-top: 20%;">
            <img src="~/Images/Error.png" style="margin-left: 0;">
            <samp style="color: white; font-size: 25px;">Activity is closed. Check status of activity in Siebel!</samp>
        </div>
    </div>
}
else if (Model.SiebelResponseAttributes.serviceRequest.activityDetailList.Count() > 0)
